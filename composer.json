{
    "name": "medialab/ScienceTrotterS_API",
    "description": "Science Trotters API.",
    "keywords": ["sts"],
    "license": "GPLv2",
    "type": "project",
    "authors": [
        {
            "name": "The net group",
            "email": "contact@thenetgroup.net",
            "homepage": "http://thenetgroup.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
